<AjustesPage>: #informar o mesmo nome da classe entre <>
    FloatLayout:
        canvas: 
            Rectangle:
                size: self.size
                pos: self.pos
                source: "icones/bg1.png"
        Label:
            canvas.before:
                Rectangle:
                    pos: self.pos
                    size: self.size
                    source: "icones/botao_fino.png"
            pos_hint: {"right": 0.85, "top":0.95}
            size_hint: 0.7, 0.2
            text: "Seu ID Único: 1"
            bold: True #negrito igual a True
            id: id_vendedor
       
        #Mudar foto de perfil
        ImageButton:
            pos_hint: {"right": 0.5, "top":0.7}
            size_hint: 0.5, 0.15
            source: "icones/foto_perfil.png"
            on_release: app.mudar_tela("fotoperfilpage") #on_release estara vinculada a uma função para mudar de tela
        LabelButton:
            pos_hint: {"right": 0.9, "top":0.7}
            size_hint: 0.5, 0.15
            text: "Mudar Foto de Perfil"
            on_release: app.mudar_tela("fotoperfilpage") #on_release estara vinculada a uma função para mudar de tela

        #Adicionar o Vendedor
        ImageButton:
            pos_hint: {"right": 0.5, "top":0.5}
            size_hint: 0.5, 0.15
            source: "icones/adicionar_vendedor.png"
            on_release: app.mudar_tela("adicionarvendedorpage")
        LabelButton:
            pos_hint: {"right": 0.9, "top":0.5}
            size_hint: 0.5, 0.15
            text: "Adicionar o Vendedor"
            on_release: app.mudar_tela("adicionarvendedorpage")

        #Ver todas as vendas
        ImageButton:
            pos_hint: {"right": 0.5, "top":0.3}
            size_hint: 0.5, 0.15
            source: "icones/vendas_empresa.png"
            on_release: app.carregar_todas_vendas() #para carregar as infos somente quando acessar a tela
        LabelButton:
            pos_hint: {"right": 0.9, "top":0.3}
            size_hint: 0.5, 0.15
            text: "Ver todas as vendas"
            on_release: app.carregar_todas_vendas() #para carregar as infos somente quando acessar a tela

        #Botão Voltar
        FloatLayout:
            pos_hint: {"right": 1, "top":0.12}
            size_hint: 1, 0.12
            canvas: 
                Color:
                    rgb: utils.get_color_from_hex("#006D7E")
                Rectangle:
                    size: self.size
                    pos: self.pos
            ImageButton:
                pos_hint: {"right": 0.2, "top":0.9}
                size_hint: 0.2, 0.8
                source: "icones/voltar.png"
                on_release: app.mudar_tela("homepage") #on_release estara vinculada a uma função para mudar de tela